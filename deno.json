{
  "name": "@m4rc3l05/denotag",
  "version": "4.6.4",
  "exports": "./src/main.ts",
  "tasks": {
    "run": "./scripts/bundle.ts && deno run --allow-read=./data --cached-only --allow-env=ENV --allow-net=127.0.0.1 src/main.ts",
    "compile": "./scripts/bundle.ts && deno compile --allow-read=./data --cached-only --allow-env=ENV --allow-net=127.0.0.1 --include ./data/index.html --env=.env -o ./.bin/denotag ./src/main.ts"
  },
  "compilerOptions": {
    "lib": ["DOM", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "lint": {
    "exclude": ["embed.json"],
    "rules": {
      "include": ["verbatim-module-syntax"]
    }
  },
  "fmt": {
    "exclude": ["embed.json", "data"]
  },
  "lock": {
    "frozen": true
  },
  "imports": {
    "@david/dax": "jsr:@david/dax@0.42.0",
    "@luca/esbuild-deno-loader": "jsr:@luca/esbuild-deno-loader@0.11.0",
    "@mjackson/multipart-parser": "jsr:@mjackson/multipart-parser@0.6.2",
    "@std/cli": "jsr:@std/cli@1.0.7",
    "@std/path": "jsr:@std/path@1.0.8",
    "esbuild": "npm:esbuild@0.24.0",
    "node-taglib-sharp": "npm:node-taglib-sharp@6.0.0",
    "react": "npm:react@18.3.1",
    "react-bootstrap": "npm:react-bootstrap@2.10.6",
    "react-dom": "npm:react-dom@18.3.1"
  }
}
