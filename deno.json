{
  "name": "@m4rc3l05/denotag",
  "version": "4.4.0",
  "exports": "./src/main.ts",
  "tasks": {
    "run": "./scripts/public-to-json.ts && deno run --allow-env=ENV --allow-net=127.0.0.1 --cached-only src/main.ts",
    "compile": "./scripts/public-to-json.ts && deno compile --allow-env=ENV --allow-net=127.0.0.1 --unstable-ffi --include ./src/public.json --cached-only --env=.env -o ./.bin/denotag ./src/main.ts",
    "deps:lock": "deno cache --frozen=false **/*.ts **/*.tsx",
    "deps": "deno cache --reload **/*.ts **/*.tsx"
  },
  "compilerOptions": {
    "lib": ["DOM", "deno.ns"],
    "jsx": "react-jsx"
  },
  "lint": {
    "rules": {
      "include": ["verbatim-module-syntax"]
    }
  },
  "fmt": {
    "exclude": ["./src/public.json"]
  },
  "imports": {
    "@luca/esbuild-deno-loader": "jsr:@luca/esbuild-deno-loader@0.10.3",
    "@mjackson/multipart-parser": "jsr:@mjackson/multipart-parser@0.6.2",
    "@std/path": "jsr:@std/path@1.0.6",
    "cliffy": "https://deno.land/x/cliffy@v1.0.0-rc.4/command/mod.ts",
    "esbuild": "npm:esbuild@0.23.1",
    "node-taglib-sharp": "npm:node-taglib-sharp@5.2.3",
    "react": "npm:react@18.3.1",
    "react-bootstrap": "npm:react-bootstrap@2.10.4",
    "react-dom": "npm:react-dom@18.3.1",
    "react-hotkeys-hook": "npm:react-hotkeys-hook@4.5.0"
  }
}
